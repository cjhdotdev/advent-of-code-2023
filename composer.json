{
    "name": "cjhdotdev/advent-of-code-2023",
    "description": "Advent of Code 2023",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris",
            "email": "chris@cjh.dev"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel-zero/framework": "^10.3.0",
        "nunomaduro/termwind": "^1.15.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13.6",
        "mockery/mockery": "^1.6",
        "nunomaduro/larastan": "^2.6",
        "pestphp/pest": "^2.25.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["aoc2023"]
}
